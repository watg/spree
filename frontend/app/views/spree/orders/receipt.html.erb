<fieldset id="order_summary" data-hook>  
  <h1><%= accurate_title %></h1>

  <div id="order" data-hook>
    <%= render :partial => 'linkshare_conversion_tag' %>
    <%= render :partial => 'google_adword_tag' %>
    <%= render :partial => 'adroll_conversion_tag' %>
    <%= render :partial => 'facebook_conversion_tag', :locals => { :order => @order } %>
    <%= render :partial => 'olapic_conversion_tag', :locals => { :order => @order } %>

    <%= render :partial => 'spree/shared/order_details', :locals => { :order => @order } %>

    <br class="clear" />

    <%= render :partial => 'links', :locals => { :order => @order } %>

  </div>
</fieldset>
