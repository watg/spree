<%= render :partial => 'spree/shared/error_messages', :locals => { :target => @product_group } %>

<%= form_for [:admin, @product_group] do |f| %>
  <fieldset data-hook="admin_product_group_new_form">
    <legend align="center"><%= Spree.t(:new_product_group) %></legend>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= render :partial => 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>

